    "babel-loader": "8.2.2",
    "concurrently": "6.2.2",
    "copy-webpack-plugin": "9.1.0",
    "cross-env": "7.0.3",
    "electron": "git+https://github.com/castlabs/electron-releases.git#fe1bf6a",
    "electron-builder": "22.14.13",
    "electron-extensions": "git+https://github.com/Alex313031/electron-extensions.git#01f686b",
    "electron-updater": "4.3.9",
