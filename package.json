{
  "name": "promethium",
  "appName": "promethium",
  "productName": "Promethium",
  "applicationId": "com.alex313031.promethium",
  "version": "6.1.7",
  "sideEffects": false,
  "main": "build/main.bundle.js",
  "description": "Extensible, fast, and innovative Electron based web browser with Material UI design elements.",
  "keywords": [
    "web-browser",
    "material",
    "material-ui",
    "electron",
    "chromium",
    "react",
    "mobx",
    "styled-components"
  ],
  "homepage": "https://github.com/Alex313031/promethium#readme",
  "license": "GPL-3.0+",
  "author": "Alex313031 <alex313031@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alex313031/promethium.git"
  },
  "bugs": {
    "url": "https://github.com/Alex313031/promethium/issues"
  },
  "scripts": {
    "init": "cd promethium-rpc && yarn install",
    "dev-renderer": "cross-env DEV=1 webpack serve --config webpack.config.renderer.js",
    "dev-webpack": "cross-env DEV=1 webpack",
    "build-renderer": "webpack --config webpack.config.renderer.js",
    "dev": "cross-env START=1 npm run watch",
    "build": "rimraf build && concurrently \"npm run build-renderer\" \"webpack\"",
    "ci-build": "node scripts/ci-build.js",
    "start": "npm run build && electron .",
    "e": "electron .",
    "clean": "rimraf build && rimraf dist && rimraf node_modules",
    "distclean": "rimraf build && rimraf dist",
    "watch": "concurrently \"npm run dev-renderer\" \"npm run dev-webpack\"",
    "distWin": "npm run build && electron-builder -w --pd dist/win-unpacked",
    "distMac": "npm run build && electron-builder -m --pd dist/mac-unpacked",
    "distLinux": "npm run build && electron-builder -l",
    "dist": "npm run build && electron-builder --dir",
    "lint": "eslint \"src/**/*.ts*\" \"src/**/*.tsx*\"",
    "lint-fix": "npm run lint -- --fix",
    "rebuild": "electron-builder install-app-deps",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "asar": "3.2.0",
    "@babel/core": "7.17.12",
